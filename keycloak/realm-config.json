{
  "realm": "demo",
  "enabled": true,
  "displayName": "Demo Realm",
  "loginTheme": "acme-theme",
  "users": [
    {
      "username": "testuser",
      "enabled": true,
      "credentials": [
        {
          "type": "password",
          "value": "testpassword",
          "temporary": false
        }
      ],
      "email": "testuser@example.com",
      "firstName": "Test",
      "lastName": "User"
    }
  ],
  "clients": [
    {
      "clientId": "quarkus-app",
      "enabled": true,
      "clientAuthenticatorType": "client-secret",
      "secret": "your-client-secret-here",
      "redirectUris": [
        "https://localhost:8443/redirect_uri",
        "https://localhost:8443/user",
        "https://localhost:8443/logged-out"
      ],
      "webOrigins": [
        "http://localhost:8080",
        "https://localhost:8443"
      ],
      "protocol": "openid-connect",
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": true,
      "serviceAccountsEnabled": false,
      "publicClient": false,
      "consentRequired": false,
      "defaultClientScopes": [
        "web-origins",
        "role_list",
        "profile",
        "roles",
        "email"
      ],
      "optionalClientScopes": [
        "address",
        "phone",
        "offline_access",
        "microprofile-jwt"
      ]
    }
  ],
  "defaultDefaultClientScopes": [
    "role_list",
    "profile",
    "email",
    "roles",
    "web-origins"
  ],
  "defaultOptionalClientScopes": [
    "offline_access",
    "address",
    "phone",
    "microprofile-jwt"
  ]
}

